{
	"name": "myreads",
	"version": "1.0.0",
	"dependencies": {
		"ajv": "^6.4.0",
		"bootstrap": "4.0.0",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"jquery": "3.0.0",
		"node-sass": "^4.8.3",
		"npm-run-all": "^4.1.2",
		"popper.js": "^1.12.9",
		"prop-types": "^15.5.8",
		"react": "^16.3.2",
		"react-debounce-input": "^3.2.0",
		"react-dom": "^16.0.0",
		"react-loading": "^2.0.2",
		"react-router-dom": "^4.2.2",
		"react-stars": "^2.2.5",
		"reactstrap": "5.0.0"
	},
	"devDependencies": {
		"react-scripts": "^1.0.0"
	},
	"scripts": {
		"build-js": "NODE_PATH=src react-scripts build",
		"start-js": "NODE_PATH=src react-scripts start",
		"watch-scss": "node-sass --watch ./src/styles/scss -o ./src/styles/css",
		"build-scss": "node-sass ./src/styles/scss -o ./src/styles/css",
		"build": "npm-run-all -p build-scss build-js",
		"start": "npm-run-all -p watch-scss start-js",
		"test": "NODE_PATH=src react-scripts test --env=jsdom",
		"eject": "react-scripts eject"
	}
}
